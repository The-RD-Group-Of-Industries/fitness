{
  "expo": {
    "name": "Fitness Evolution",
    "slug": "fitness",
    "version": "8.1.1",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fitness.evolution",
      "icon": "./assets/images/icon.png",
      "buildNumber": "8.1.0",     
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "hardwareAccelerated": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fitness.evolution"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "webConfig": {
      "favicon": "./assets/images/favicon.png",
      "manifest": {
        "short_name": "Fitness Evolution",
        "name": "Fitness Evolution",
        "start_url": "/",
        "display": "standalone",
        "background_color": "#ffffff",
        "theme_color": "#000000",
        "description": "Your fitness companion PWA",
        "icons": [
          {
            "src": "./assets/images/icon.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./assets/images/adaptive-icon.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2fa4e060-839d-4d63-98dc-a6a9c071b364"
      }
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}
